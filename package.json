{
  "name": "nuisancevalue",
  "version": "0.0.1",
  "engines": {
    "node": ">=11.0.0"
  },
  "description": "An HTML5 multiplayer bridge simulator game based on Lance, P2 and PIXI",
  "main": "main.js",
  "dependencies": {
    "@babel/node": "^7.7.4",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "express": "^4.17.1",
    "lance-gg": "^4.0.8",
    "pixi.js": "^5.2.0",
    "query-string": "^4.2.3",
    "socket.io": "^2.3.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.7.1"
  },
  "scripts": {
    "start": "node dist-server/main.js",
    "start-debug": "node --inspect-brk dist-server/main.js",
    "start-dev": "nodemon dist-server/main.js",
    "postinstall": "webpack && babel src --source-maps --out-dir dist-server",
    "build": "webpack && babel src --source-maps --out-dir dist-server",
    "stats": "webpack --env production --profile --json > stats.json",
    "dev": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "game",
    "bridge-simulator",
    "lance",
    "lance-gg",
    "multiplayer",
    "websockets"
  ],
  "author": "Jim Rowe",
